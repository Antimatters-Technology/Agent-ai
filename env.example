# VisaMate AI Environment Configuration

# Application Settings
APP_NAME=VisaMate AI
API_VERSION=v1
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=info

# AWS Configuration (for production)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key-here
AWS_SECRET_ACCESS_KEY=your-secret-access-key-here

# AWS Cognito Authentication
COGNITO_USER_POOL_ID=us-east-1_xxxxxxxxx
COGNITO_CLIENT_ID=your-cognito-client-id
COGNITO_CLIENT_SECRET=your-cognito-client-secret
JWT_SECRET=your-jwt-secret-key-change-in-production
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# AWS Services
RDS_ENDPOINT=your-rds-endpoint.amazonaws.com
RDS_DATABASE=visamate
RDS_USERNAME=admin
RDS_PASSWORD=your-rds-password
DATABASE_URL=postgresql://admin:password@localhost:5432/visamate

DYNAMODB_TABLE_PREFIX=visamate
S3_BUCKET_NAME=visamate-documents

# Google Gemini AI
GEMINI_API_KEY=your-gemini-api-key
GEMINI_MODEL=gemini-1.5-pro
GEMINI_TEMPERATURE=0.7
GEMINI_MAX_TOKENS=8192

# Alternative AI Services (optional)
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key

# Security
ENCRYPTION_KEY=your-32-character-encryption-key-here
CORS_ORIGINS=http://localhost:3000,http://localhost:8080

# Payment Integration (optional)
STRIPE_SECRET_KEY=sk_test_...
STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# WhatsApp Integration (optional)
WHATSAPP_TOKEN=your-whatsapp-token
WHATSAPP_VERIFY_TOKEN=your-verify-token
WHATSAPP_PHONE_NUMBER_ID=your-phone-number-id

# Monitoring (optional)
SENTRY_DSN=your-sentry-dsn
GOOGLE_ANALYTICS_ID=your-ga-id

# Development Settings
RATE_LIMIT_PER_MINUTE=60
SOP_MIN_WORDS=800
SOP_MAX_WORDS=1500
MAX_FILE_SIZE_MB=10 